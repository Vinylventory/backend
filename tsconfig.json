{
  "include": [
    "src",
    "prisma/seed.ts"
  ],
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "lib": [
      "esnext.asynciterable",
      "ES2021",
      "DOM",
    ],
    "paths": {
      "@/*": ["./src/*"]
    },
    "outDir": "dist",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
  }
}